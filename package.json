{
  "name": "angular-workshop-seed",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:github-page": "ng build --prod --output-path docs --base-href /angular-workshop-seed/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "ng test --progress=false --watch=false --code-coverage",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "extract": "ng xi18n --output-path src/locale",
    "serve:de:aot": "ng serve -prod --locale de --i18n-format xlf --i18n-file src/locale/messages.de.xlf",
    "build:en:aot": "ng build -prod --output-path=dist-de --locale de --i18n-format xlf --i18n-file src/locale/messages.de.xlf"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.14.3",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular/cli": "^8.3.24",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~13.7.0",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "^3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
